  "algorithmicArt": {
    "wood": {
      "type": "organic_growth",
      "particles": 3000,
      "colors": ["#10B981", "#34D399", "#6EE7B7"],
      "movement": "branching",
      "speed": 0.5,
      "description": "Branching growth patterns resembling trees and vines"
    },
    "fire": {
      "type": "flow_field",
      "particles": 5000,
      "colors": ["#EF4444", "#F97316", "#FBBF24"],
      "movement": "turbulent",
      "speed": 0.8,
      "description": "Dynamic flow field with flame-like turbulence"
    },
    "earth": {
      "type": "particle_grid",
      "particles": 2000,
      "colors": ["#F59E0B", "#D97706", "#92400E"],
      "movement": "stable",
      "speed": 0.2,
      "description": "Stable grid patterns with subtle shifting like tectonic plates"
    },
    "metal": {
      "type": "geometric_pattern",
      "particles": 1500,
      "colors": ["#6B7280", "#9CA3AF", "#D1D5DB"],
      "movement": "crystalline",
      "speed": 0.3,
      "description": "Sharp geometric patterns resembling metal crystals"
    },
    "water": {
      "type": "wave_pattern",
      "particles": 4000,
      "colors": ["#3B82F6", "#0EA5E9", "#38BDF8"],
      "movement": "flowing",
      "speed": 0.6,
      "description": "Flowing wave patterns with ripple effects"
    }
  },
  "accessibility": {
    "focusRing": "2px solid #8B5CF6",
    "focusRingOffset": "2px",
    "minContrastRatio": 4.5,
    "reducedMotion": true
  }
}
